# Environment Configuration
VITE_APP_NAME="E Learning Platform"
VITE_API_BASE_URL=http://localhost:3001/api
VITE_APP_VERSION=1.0.0

# Feature Toggles
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_LIVE_LECTURES=true
VITE_ENABLE_YOUTUBE_INTEGRATION=true
VITE_ENABLE_ZOOM_INTEGRATION=false

# External API Keys (Optional)
VITE_YOUTUBE_API_KEY=your_youtube_api_key_here
VITE_ZOOM_API_KEY=your_zoom_api_key_here

# JWT Configuration
VITE_JWT_TOKEN_KEY=auth_token
VITE_JWT_REFRESH_KEY=refresh_token

# File Upload Configuration
VITE_MAX_FILE_SIZE=50MB
VITE_ALLOWED_VIDEO_FORMATS=mp4,webm,ogg
VITE_ALLOWED_DOCUMENT_FORMATS=pdf,doc,docx,ppt,pptx
